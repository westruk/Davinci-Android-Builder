[
  {
    "kernelSource": {
      "name": "InfinitykKernel",
      "repo": "github.com/westruk/Infinity_mod.git", #without https
      "branch": "12.1",
      "device": "davinci",
      "defconfig": "davinci_defconfig"
    },
    "toolchains": [
      {
        "name": "proton-clang",
        "repo": "https://github.com/kdrag0n/proton-clang",
        "branch": "master",
        "binaryEnv": ["./bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "proton-clang/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "proton-clang/bin/arm-linux-gnueabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
   "use": true,
   "release": true,
   "custom": {
     "repo": "https://github.com/westruk/AnyKernel3_davinci",
     "branch": "master"
      }
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
